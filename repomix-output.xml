  useEffect(() => {
    if (isAuthenticated !== 'authenticated') {
      if (adminUser?.role !== 'administrator') {
        return navigate('/');
      }
    }
  }, [isAuthenticated, adminUser?.role, navigate]);

  useEffect(() => {
    const fetchData = async () => {
      try {
        const [userResponse, categoryResponse, priceResponse] =
